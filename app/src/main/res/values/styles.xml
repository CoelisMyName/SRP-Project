<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ListedCardViewStyle" parent="CardView">
        <item name="android:layout_margin">4dp</item>
        <item name="cardCornerRadius">8dp</item>
        <item name="cardElevation">4dp</item>
        <item name="contentPadding">8dp</item>
    </style>

    <style name="ContentCardViewStyle" parent="Widget.MaterialComponents.CardView">
        <item name="android:layout_marginHorizontal">3dp</item>
        <item name="android:layout_marginVertical">6dp</item>
        <item name="cardCornerRadius">12dp</item>
        <item name="cardElevation">2dp</item>
        <item name="contentPadding">10dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="cardBackgroundColor">@color/foreground</item>
    </style>

    <style name="ContentCardViewTitleStyle" parent="Widget.AppCompat.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/text_main</item>
        <item name="android:layout_marginBottom">3dp</item>
    </style>

    <style name="ContentCardViewMainStyle" parent="Widget.AppCompat.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/text_aux</item>
        <item name="android:textSize">18sp</item>
        <item name="android:lineHeight">22.5sp</item>
    </style>

    <style name="ContentCardViewMainStyleNewParam" parent="ContentCardViewMainStyle">
        <item name="android:layout_marginTop">10sp</item>
    </style>

    <style name="ContentCardViewLayoutStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingHorizontal">5dp</item>
    </style>

    <style name="Navigator" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textSize">20sp</item>
    </style>

    <style name="CustomizeTitle" parent="Widget.AppCompat.TextView">
        <item name="android:layout_margin">20dp</item>
        <item name="android:textSize">24sp</item>
    </style>

    <style name="CustomizeBack" parent="Widget.AppCompat.ImageButton">
        <item name="android:layout_marginStart">20dp</item>
        <item name="android:layout_width">26sp</item>
        <item name="android:layout_height">26sp</item>
        <item name="android:background">@color/none</item>
        <item name="android:backgroundTint">@color/on_activate</item>
        <item name="android:padding">0dp</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:minHeight">0dp</item>
        <item name="android:src">@drawable/back_button</item>
        <item name="android:textSize">20sp</item>
        <item name="contentDescription">@string/back</item>
    </style>

    <style name="IndependentCustomizeTitle_CardView" parent="Widget.MaterialComponents.CardView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="cardCornerRadius">0dp</item>
        <item name="cardElevation">3dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="contentPadding">0dp</item>
        <item name="cardBackgroundColor">@color/foreground</item>
    </style>

    <style name="IndependentCustomizeTitle_Layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">20dp</item>
        <item name="android:paddingBottom">14dp</item>
    </style>

    <style name="IndependentCustomizeTitle_Text" parent="CustomizeTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">0dp</item>
        <item name="android:textColor">@color/text_main</item>
    </style>

    <style name="AboutPage_MoreInfoButton" parent="Widget.MaterialComponents.Button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20sp</item>
        <item name="android:minHeight">0dp</item>
        <item name="android:background">@color/none</item>
        <item name="android:backgroundTint">@color/on_activate</item>
        <item name="android:padding">0dp</item>
        <item name="android:textColor">@color/text_main_strong</item>
    </style>

    <style name="EssRatingBarStyle" parent="Widget.AppCompat.RatingBar.Indicator">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:numStars">4</item>
        <item name="android:progressBackgroundTint">@color/darkerBackground</item>
        <item name="android:progressTint">@color/text_main_strong</item>
        <item name="android:rating">1</item>
        <item name="android:secondaryProgressTint">@color/none</item>
        <item name="android:stepSize">1</item>
        <item name="android:layout_marginEnd">8dp</item>
        <item name="android:isIndicator">false</item>
    </style>

    <style name="EssRatingBarLayoutStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">start|center_vertical</item>
    </style>

    <style name="HorizontalSepLineStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/background</item>
    </style>

    <style name="VerticalSepLineStyle">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/background</item>
    </style>

    <style name="HorizontalSepLineStyle_Margin20" parent="HorizontalSepLineStyle">
        <item name="android:layout_marginVertical">20dp</item>
    </style>

    <style name="HorizontalSepLineStyle_Margin10" parent="HorizontalSepLineStyle">
        <item name="android:layout_marginVertical">10dp</item>
    </style>

    <style name="VerticalSepLineStyle_Margin10" parent="VerticalSepLineStyle">
        <item name="android:layout_marginHorizontal">10dp</item>
    </style>

    <style name="PsqiQuestionTextStyle" parent="ContentCardViewMainStyle">
        <item name="android:textColor">@color/text_main</item>
    </style>

    <style name="PsqiSelectorStyle" parent="Widget.AppCompat.CompoundButton.RadioButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/text_main</item>
        <item name="android:textSize">18sp</item>
        <item name="android:minHeight">0dp</item>
        <item name="android:layout_marginVertical">3dp</item>
        <item name="android:layout_marginStart">12dp</item>
    </style>

    <style name="PsqiSelectorStyleCommon1" parent="PsqiSelectorStyle">
        <item name="android:text">@string/psqi_test_Common_Frequence_1</item>
    </style>

    <style name="PsqiSelectorStyleCommon2" parent="PsqiSelectorStyle">
        <item name="android:text">@string/psqi_test_Common_Frequence_2</item>
    </style>

    <style name="PsqiSelectorStyleCommon3" parent="PsqiSelectorStyle">
        <item name="android:text">@string/psqi_test_Common_Frequence_3</item>
    </style>

    <style name="PsqiSelectorStyleCommon4" parent="PsqiSelectorStyle">
        <item name="android:text">@string/psqi_test_Common_Frequence_4</item>
    </style>

    <style name="PsqiScoreText" parent="Widget.AppCompat.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">5sp</item>
        <item name="android:textColor">@color/text_main</item>
        <item name="android:textSize">26sp</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="PsqiScoreTextWithPadding" parent="PsqiScoreText">
        <item name="android:paddingTop">20sp</item>
    </style>
    <style name="PsqiScoreNoteText" parent="Widget.AppCompat.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/text_aux</item>
        <item name="android:textSize">18sp</item>
    </style>
    <style name="PsqiScoreNoteTextPart" parent="PsqiScoreNoteText">
        <item name="android:text">@string/psqi_test_partScore</item>
    </style>
    <style name="MinVerticalLinearLayoutCenter">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="PsqiPartDescribeLayout_Packer">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="PsqiPartDescribeLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="PsqiPartDescribeLayout_Left">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_weight">1</item>
    </style>
</resources>